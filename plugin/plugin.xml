<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
xmlns:android="http://schemas.android.com/apk/res/android"
id="cordova-plugin-nearbeeplugin" version="0.0.1">
    <name>NearbeePlugin</name>
    <description>An Android Cordova plugin that allows users to receive notifications from beacons.</description>
    <keywords>cordova,android,nearbee,beacons,alert</keywords>
    <repo>https://_anuragbansal@bitbucket.org/_anuragbansal/cordova-nearbee-sdk.git</repo>
    <engines>
        <engine name="cordova" version=">=3.0.0"/>
    </engines>
    <js-module src="www/nearbeeplugin.js" name="nearbeePlugin">
        <clobbers target="window.plugins.nearbeePlugin" />
    </js-module>
    <framework src="co.nearbee:nearbeesdk:1.1.4" />
    <platform name="android">
        <config-file target="config.xml" parent="/*">
            <feature name="NearbeePlugin">
                <param name="android-package" value="com.mobstac.cordova.plugin.NearbeePlugin"/>
            </feature>
        </config-file>
        <source-file src="src/android/NearbeePlugin.java" target-dir="src/com/mobstac/cordova/plugin" />
    </platform>
    <platform name="ios">
        <config-file target="config.xml" parent="/*">
            <feature name="NearbeePlugin">
                <param name="ios-package" value="NearbeePlugin" />
            </feature>
        </config-file>
        <framework src="NearBee" type="podspec" spec="0.1.0" />
        <header-file src="src/ios/NearbeePlugin.h" />
        <source-file src="src/ios/NearbeePlugin.m" />
    </platform>
</plugin>